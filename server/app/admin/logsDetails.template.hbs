<h1>Log</h1>
<a href="/logs">back to logs</a>

<div class="form-group row">
	<label class="col-2 col-form-label">Level</label>
	<div class="col-10 form-control-text">
		{{logLevel}}
	</div>
</div>
<div class="form-group row">
	<label class="col-2 col-form-label">Message</label>
	<div class="col-10 form-control-text">
		<strong>{{logMessage}}</strong>
		{{#if stack}}<br>{{stack}}{{/if}}
	</div>
</div>
<div class="form-group row">
	<label class="col-2 col-form-label">Status</label>
	<div class="col-10 form-control-text">
		{{httpStatus}}
	</div>
</div>
<div class="form-group row">
	<label class="col-2 col-form-label">Url</label>
	<div class="col-10 form-control-text">
		{{requestUrl}}
	</div>
</div>
<div class="form-group row">
	<label class="col-2 col-form-label">Username</label>
	<div class="col-10 form-control-text">
		{{username}}
	</div>
</div>